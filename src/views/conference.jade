        // Sidebar
        .col-lg-3.col-md-3.col-sm-4.col-lg-pull-9.col-md-pull-9.col-sm-pull-8.sidebar
          h3(style='margin-top:0') 年份
          // Accordions
          ul.accordions
            for items, catogory in list_items
              // Accordion
              li.accordion
                .accordion-header
                  .accordion-icon
                  h6 #{catogory}
                .accordion-content
                  ul.list.finger-list
                    for item in items
                      li
                        a(href=item.link) #{item.title}
              // /Accordion
          .social-media.animate-onscroll
            span.small-caption Get connected:
            ul.social-icons
              li.facebook
                a.tooltip-ontop(href='#', title='Facebook')
                  i.icons.icon-facebook
              li.twitter
                a.tooltip-ontop(href='#', title='Twitter')
                  i.icons.icon-twitter
              li.google
